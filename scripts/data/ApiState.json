{
  "types": {
    "B": {
      "description": "boolean",
      "ctype": "GLboolean"
    },
    "BMU": {
      "description": "basic machine units",
      "ctype": "GLubyte"
    },
    "C": {
      "description": "color (floating point RGBA)",
      "ctype": "GLfloat",
      "tuple-size": 4
    },
    "E": {
      "description": "enumerated value",
      "ctype": "GLenum"
    },
    "CI": {
      "description": "color index (floating point)",
      "ctype": "GLfloat"
    },
    "T": {
      "description": "texture coordinate (floating point STRQ)",
      "ctype": "GLfloat",
      "tuple-size" : 4
    },
    "N": {
      "description": "normal coordinate (floating point XYZ)",
      "ctype": "GLfloat",
      "tuple-size": 3
    },
    "V": {
      "description": "vertex, including associated data",
      "ctype": "vertex"
    },
    "Z": {
      "description": "integer",
      "ctype": "GLint"
    },
    "Z\+": {
      "description": "unsigned integer",
      "ctype": "GLuint"
    },
    "Z\d+": {
      "description": "unsigned integer with k values",
      "ctype": "GLuint",
      "params": [
        {
          "description": "num values",
          "value": 0
        }
      ]
    },
    "Z\d+\*": {
      "description": "unsigned integer with at least k values",
      "ctype": "GLuint",
      "params": [
        {
          "description": "num values",
          "value": 0
        }
      ]
    },
    "R": {
      "description": "floating point number",
      "ctype": "GLfloat"
    },
    "R\+": {
      "description": "unsigned floating point number",
      "ctype": "GLfloat"
    },
    "R\[\d+,\d+\]": {
      "description": "floating point number in the range [a,b]",
      "ctype": "GLfloat",
      "params": [
        {
          "description": "lower bound",
          "value": 0
        },
        {
          "description": "upper bound",
          "value": 0
        }
      ]
    },
    "R\d+": {
      "description": "k-tuple of floating point numbers",
      "ctype": "GLfloat",
      "params": [
        {
          "description": "num values",
          "value": 0
        }
      ]
    },
    "P": {
      "description": "position (floating point XYZW)",
      "ctype": "GLfloat",
      "tuple-size": 4
    },
    "D": {
      "description": "direction (floating point XYZ)",
      "ctype": "GLfloat",
      "tuple-size": 3
    },
    "M4": {
      "description": "4 x 4 floating point matrix",
      "ctype": "GLfloat",
      "tuple-size": 16
    },
    "S": {
      "description": "null-terminated string",
      "ctype": "string"
    },
    "I": {
      "description": "image",
      "ctype": "image"
    },
    "A": {
      "description": "attribute stack entry, including mask",
      "ctype": "AttributeStackEntry"
    },
    "Y": {
      "description": "pointer",
      "ctype": "GLvoid *"
    }
